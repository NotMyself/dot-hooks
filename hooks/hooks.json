        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "dotnet run ${CLAUDE_PLUGIN_ROOT}/hooks/Program.cs -- session-start",
            "timeout": 30000
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "matcher": "*",
